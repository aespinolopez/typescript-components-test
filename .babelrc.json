{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": 3,
        "targets": "supports es6-module",
        "modules": false
      }
    ],
    [
      "@babel/preset-react",
      { "runtime": "automatic", "importSource": "@emotion/react" }
    ],
    [
      "@babel/preset-typescript",
      { "isTSX": true, "jsxPragma": "@emotion/react" }
    ]
  ],
  "plugins": ["@emotion"],
  "ignore": ["**/*.stories.tsx"],
  "sourceMaps": true,
  "sourceType": "module"
}
